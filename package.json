{
  "name": "samples-3d",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:app": "pnpm --filter=@lizhou/3d-samples run dev",
    "build:app": "pnpm --filter=@lizhou/3d-samples run build",
